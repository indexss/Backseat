<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity Album.
    -->
    <changeSet id="20240226152041-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="album_id"
                                 baseTableName="rel_album__artist"
                                 constraintName="fk_rel_album__artist__album_id"
                                 referencedColumnNames="id"
                                 referencedTableName="album"/>

        <addForeignKeyConstraint baseColumnNames="artist_id"
                                 baseTableName="rel_album__artist"
                                 constraintName="fk_rel_album__artist__artist_id"
                                 referencedColumnNames="id"
                                 referencedTableName="artist"/>
    </changeSet>
</databaseChangeLog>
