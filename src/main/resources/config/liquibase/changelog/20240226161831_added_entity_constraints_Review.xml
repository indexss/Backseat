<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity Review.
    -->
    <changeSet id="20240226161831-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="profile_id"
                                 baseTableName="review"
                                 constraintName="fk_review__profile_id"
                                 referencedColumnNames="id"
                                 referencedTableName="profile"/>

        <addForeignKeyConstraint baseColumnNames="track_id"
                                 baseTableName="review"
                                 constraintName="fk_review__track_id"
                                 referencedColumnNames="id"
                                 referencedTableName="track"/>

        <addForeignKeyConstraint baseColumnNames="album_id"
                                 baseTableName="review"
                                 constraintName="fk_review__album_id"
                                 referencedColumnNames="id"
                                 referencedTableName="album"/>
    </changeSet>
</databaseChangeLog>
