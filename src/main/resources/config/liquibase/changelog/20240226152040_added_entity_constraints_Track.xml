<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity Track.
    -->
    <changeSet id="20240226152040-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="track_id"
                                 baseTableName="rel_track__artist"
                                 constraintName="fk_rel_track__artist__track_id"
                                 referencedColumnNames="id"
                                 referencedTableName="track"/>

        <addForeignKeyConstraint baseColumnNames="artist_id"
                                 baseTableName="rel_track__artist"
                                 constraintName="fk_rel_track__artist__artist_id"
                                 referencedColumnNames="id"
                                 referencedTableName="artist"/>

        <addForeignKeyConstraint baseColumnNames="album_id"
                                 baseTableName="track"
                                 constraintName="fk_track__album_id"
                                 referencedColumnNames="id"
                                 referencedTableName="album"/>
    </changeSet>
</databaseChangeLog>
